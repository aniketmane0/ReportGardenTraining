1.CREATE TABLE CustomersNew(id int primary key,  name text, email text, address text);
  CREATE TABLE itemsNew  (item_id int primary key,  item_name text, price text );
  COPY CustomersNew from '/home/user/customers.csv' delimiter ',' csv;
  COPY itemsNew from '/home/user/items.csv' delimiter ',' csv;
2.ALTER TABLE CustomersNew ADD CONSTRAIBNT MyUniqueConstraint unique(email);
3.ALTER TABLE itemsNew ADD CONSTRAINT MyUnique unique(item_name);
  ALTER TABLE itemsNew ALTER COLUMN price set NOT NULL;
4.SELECT id,item_name,price FROM itemsNew WHERE price>40000;
5.SELECT id,item_name,price FROM itemsNew WHERE (price>13000) AND (price<50000);
6.SELECT id,item_name,price FROM itemsNew WHERE price<12000;
